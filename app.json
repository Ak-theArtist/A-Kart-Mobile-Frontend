{
  "expo": {
    "name": "A-Kart",
    "slug": "a-kart-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/assets/logo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.akart.mobile",
      "icon": "./public/assets/logo.jpg"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/assets/logo.jpg",
        "backgroundColor": "#ffffff",
        "foregroundImageScale": 0.75
      },
      "package": "com.akart.mobile",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./public/assets/logo.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./public/assets/logo.jpg",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "imageResizeMode": "cover"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1b620be0-85fc-4657-8a27-260b512f0863"
      }
    }
  }
}
